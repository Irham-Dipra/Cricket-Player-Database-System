<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="524.0" prefWidth="875.0" stylesheets="@testStyles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.showClubListDetailsController">
    <children>
      <ImageView fitHeight="524.0" fitWidth="875.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/background-blur.jpg" />
         </image>
      </ImageView>
        <!-- Input Area -->
        
        <!-- Table to Display Player Info -->
        <TableView fx:id="playerTable" layoutX="181.0" layoutY="39.0" prefHeight="400.0" prefWidth="512.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="120.79998779296875" />
                <TableColumn fx:id="infoColumn" prefWidth="278.3999938964844" />
            <TableColumn fx:id="actionColumn" prefWidth="100.79998779296875" />
            </columns>
        </TableView>
      <Button fx:id="back" layoutX="401.0" layoutY="454.0" mnemonicParsing="false" onAction="#backClicked" styleClass="modern-button" text="back" />
    </children>
</AnchorPane>
